{
  "name": "@interactjs/_dev",
  "version": "1.10.4",
  "private": true,
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "bootstrap": "yarn install --pure-lockfile --prefer-offline --silent",
    "start": "_bundle_shims && yarn esnext --serve",
    "bundle": "_bundle_shims && NODE_ENV=${NODE_ENV:-production} _bundle",
    "build": "yarn docs && yarn bundle && _add_plugin_indexes && _types && _esnext",
    "docs": "node jsdoc/index.js",
    "esnext": "_add_plugin_indexes && _esnext",
    "test": "jest",
    "test:debug": "TEST_RUNNER=nodemon TEST_RUNNER_ARGS='-x node --inspect' yarn test",
    "tsc_lint_test": "_add_plugin_indexes && tsc -b -f && _lint && yarn test",
    "postinstall": "bin/_link; husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/taye/ijs-plus.git"
  },
  "homepage": "https://interactjs.io/pro",
  "description": "",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.interactjs.io"
  },
  "dependencies": {
